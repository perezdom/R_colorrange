library(viridisLite)

pie(rep(1,10), col = viridisLite::plasma(10), lty = 3,main = "viridisLite::plasma")
pie(rep(1,10), col = viridisLite::cividis(10), lty = 3,main = "viridisLite::cividis")
pie(rep(1,10), col = viridisLite::inferno(10), lty = 3,main = "viridisLite::inferno")
pie(rep(1,10), col = viridisLite::magma(10), lty = 3,main = "viridisLite::magma")
pie(rep(1,10), col = viridisLite::viridis(10), lty = 3,main = "viridisLite::viridis")
